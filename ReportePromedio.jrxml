	<query language="SQL"><![CDATA[SELECT
    p.nombres || ' ' || p.apellidos AS nombre_estudiante,
    e.carrera_programa AS carrera,
    e.promedio
FROM
    soyucab.estudiante e
JOIN
    soyucab.persona p
  
    ON e.email_estudiante = p.email_persona
ORDER BY
    e.promedio DESC, p.apellidos ASC
LIMIT 10]]></query>
